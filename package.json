{
  "name": "Gliese",
  "version": "0.10.0",
  "author": "Wiljan Slofstra [wiljanslofstra@gmail.com]",
  "description": "Gliese",
  "homepage": "http://wiljanslofstra.com",
  "repository": {
    "type": "git",
    "url": "git://"
  },
  "contributors": [
    {
      "name": "Wiljan Slofstra",
      "email": "wiljanslofstra@gmail.com",
      "url": "https://www.wiljanslofstra.com/"
    }
  ],
  "scripts": {
    "sass:build": "node-sass assets/sass -o assets/build --source-map assets/build --precision 8 --output-style compressed",
    "sass:autoprefixer": "postcss -u autoprefixer -o assets/build/main.css assets/build/main.css",
    "postsass:build": "npm run sass:autoprefixer",
    "sass:watch": "nodemon -e scss --watch assets/sass -x \"npm run sass:build\"",
    "js:build": "node ./bin/run bundle",
    "start": "parallelshell \"npm run js:build\" \"npm run sass:watch\"",
    "install": "napa"
  },
  "napa": {
    "polyfills": "remy/polyfills"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.0.14",
    "browser-sync": "latest",
    "imports-loader": "latest",
    "napa": "^2.2.0",
    "node-sass": "latest",
    "nodemon": "^1.8.1",
    "parallelshell": "latest",
    "postcss-cli": "^2.3.2",
    "sass-lint": "^1.3.2",
    "semistandard-loader": "0.0.3",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "bourbon": "^4.2.3",
    "fastclick": "~1.0.6",
    "imagesloaded": "~4.0.0",
    "jquery": "^2.1.4",
    "lodash": "~3.10.1",
    "parsleyjs": "~2.2.0-rc1",
    "picturefill": "~3.0.1",
    "slick-carousel": "^1.5.8",
    "waypoints": "^4.0.0"
  }
}
