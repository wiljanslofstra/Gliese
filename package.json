{
  "name": "gliese",
  "version": "0.10.0",
  "author": "Wiljan Slofstra <wiljanslofstra@gmail.com>",
  "description": "Front-end boilerplate with a few pre-build modules for web development",
  "homepage": "https://github.com/wiljanslofstra/Gliese",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://git@github.com:wiljanslofstra/Gliese.git"
  },
  "contributors": [
    {
      "name": "Wiljan Slofstra",
      "email": "wiljanslofstra@gmail.com",
      "url": "https://www.wiljanslofstra.com/"
    }
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./node_modules/gulp/bin/gulp.js",
    "start-watch": "cross-env NODE_ENV=development node ./node_modules/gulp/bin/gulp.js watch",
    "production": "cross-env NODE_ENV=production node ./node_modules/gulp/bin/gulp.js",
    "production-watch": "cross-env NODE_ENV=production node ./node_modules/gulp/bin/gulp.js watch",
    "build:modernizr": "node ./node_modules/gulp/bin/gulp.js modernizr",
    "build:precache": "node ./node_modules/gulp/bin/gulp.js swprecache",
    "build:lodash": "lodash include=\"each,filter,map,debounce,template\" -o assets/src/javascript/vendor/lodash.custom.js",
    "analyse:eslint": "eslint assets/src/javascript/**/*.js",
    "add-module": "node ./gulp/add-module.js",
    "test": "npm run analyse:eslint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9",
    "ios >= 8"
  ],
  "sasslintConfig": "gulp/config/.sass-lint.yml",
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 2%",
              "ie >= 9",
              "ios >= 8"
            ]
          },
          "modules": false
        }
      ]
    ]
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.3",
    "es6-promise": "^4.2.4",
    "imagesloaded": "^4.1.4",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "modernizr": "^3.5.0",
    "normalize-scss": "^7.0.0",
    "parsleyjs": "^2.8.0",
    "popper.js": "^1.12.7",
    "slick-carousel": "^1.8.1",
    "whatwg-fetch": "^2.0.3",
    "yarn-install": "^0.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.5",
    "cross-env": "^5.1.3",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "exports-loader": "^0.6.4",
    "gulp": "^4.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "4.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^8.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.4",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svgstore": "^6.1.1",
    "imports-loader": "^0.7.1",
    "sw-precache": "^5.2.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
