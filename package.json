{
  "name": "Gliese",
  "version": "0.10.0",
  "author": "Wiljan Slofstra <wiljanslofstra@gmail.com>",
  "description": "Front-end boilerplate with a few pre-build modules for web development",
  "homepage": "https://github.com/wiljanslofstra/Gliese",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://git@github.com:wiljanslofstra/Gliese.git"
  },
  "contributors": [
    {
      "name": "Wiljan Slofstra",
      "email": "wiljanslofstra@gmail.com",
      "url": "https://www.wiljanslofstra.com/"
    }
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "production": "npm-run-all --parallel production:*",
    "dev:js": "webpack --env development --watch",
    "dev:sass": "cross-env NODE_ENV=development nodemon -e scss --watch assets/sass -x \"node ./bin/run sass\"",
    "production:js": "webpack --env production",
    "production:sass": "cross-env NODE_ENV=production nodemon -e scss --watch assets/sass -x \"node ./bin/run sass\"",
    "build:modernizr": "node ./bin/run modernizr",
    "build:lodash": "lodash include=\"each,filter,map,debounce,template,sortBy\" -o assets/javascript/vendor/lodash.custom.js",
    "prebuild:icons": "svgo -f assets/images/icons-src -p 3 --enable removeTitle",
    "build:icons": "spritesh -i assets/images/icons-src -o assets/images/icons/sprite.svg -v \"0 0 32 32\" --prefix icon-",
    "analyse:parker": "parker assets/build/main.css",
    "analyse:eslint": "eslint assets/javascript/**/*.js",
    "analyse:todo": "node ./bin/run leasot",
    "test": "npm run analyse:eslint"
  },
  "dependencies": {
    "autotrack": "^1.0.3",
    "bootstrap-datepicker": "^1.6.4",
    "bootstrap-sass": "^3.3.7",
    "bourbon": "^4.2.7",
    "es6-promise": "^4.0.5",
    "google-maps": "^3.2.1",
    "imagesloaded": "~4.1.1",
    "jquery": "^3.1.1",
    "jump.js": "^1.0.1",
    "lodash": "~4.16.4",
    "modernizr": "^3.3.1",
    "parsleyjs": "~2.6.0",
    "slick-carousel": "^1.6.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.16.0",
    "chalk": "^1.1.3",
    "cross-env": "^3.1.3",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "glob": "^7.1.1",
    "imports-loader": "^0.6.5",
    "leasot": "^4.0.1",
    "node-sass": "^3.11.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "postcss": "^5.2.4",
    "postcss-reporter": "^2.0.0",
    "stylelint": "^7.5.0",
    "stylelint-config-suitcss": "^8.0.0",
    "stylelint-scss": "^1.3.4",
    "webpack": "2.1.0-beta.25"
  }
}
