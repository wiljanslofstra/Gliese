{
  "name": "Gliese",
  "version": "0.10.0",
  "description": "Front-end starter kit using Sass and Webpack.",
  "license": "MIT",
  "homepage": "https://github.com/wiljanslofstra/Gliese/",
  "author": [
    {
      "name": "Wiljan Slofstra",
      "email": "wiljanslofstra@gmail.com",
      "url": "https://www.wiljanslofstra.com/"
    }
  ],
  "scripts": {
    "server": "browser-sync start --files \"assets/build/main.css\" --proxy localhost:2000 --no-open",
    "sass:watch": "node-sass -w assets/sass/main.scss -o assets/build --source-map assets/build/main.css.map --precision 8",
    "sass:lint": "sass-lint -v",
    "js:watch": "webpack-dev-server",
    "js:build": "webpack",
    "watch": "parallelshell \"npm run js:watch\" \"npm run sass:watch\"",
    "start": "parallelshell \"npm run server\" \"npm run watch\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wiljanslofstra/Gliese.git"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.0.14",
    "browser-sync": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-react": "latest",
    "hjs-webpack": "^3.1.0",
    "imports-loader": "latest",
    "node-sass": "latest",
    "parallelshell": "latest",
    "sass-lint": "^1.3.2",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "bourbon": "^4.2.3",
    "fastclick": "~1.0.6",
    "gsap": "^1.17.0",
    "imagesloaded": "~3.2.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "parsleyjs": "~2.2.0-rc1",
    "picturefill": "~3.0.1",
    "react": "latest",
    "react-dom": "latest",
    "slick-carousel": "^1.5.8"
  }
}
