{
  "name": "Gliese",
  "version": "0.10.0",
  "author": "Wiljan Slofstra <wiljanslofstra@gmail.com>",
  "description": "Front-end boilerplate with a few pre-build modules for web development",
  "homepage": "https://github.com/wiljanslofstra/Gliese",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://git@github.com:wiljanslofstra/Gliese.git"
  },
  "contributors": [
    {
      "name": "Wiljan Slofstra",
      "email": "wiljanslofstra@gmail.com",
      "url": "https://www.wiljanslofstra.com/"
    }
  ],
  "scripts": {
    "start": "npm-run-all clean --parallel dev:*",
    "production": "npm-run-all clean --parallel production:*",
    "clean": "rimraf assets/build/*",
    "dev:js": "webpack --env development --watch",
    "dev:sass": "chokidar 'assets/sass/**/*.scss' -c 'cross-env NODE_ENV=development npm run build:sass' --initial --silent",
    "production:js": "webpack --env production --watch",
    "production:sass": "chokidar 'assets/sass/**/*.scss' -c 'cross-env NODE_ENV=production npm run build:sass' --initial --silent",
    "build:sass": "node ./bin/run sass",
    "build:icons": "node ./bin/run icons",
    "build:modernizr": "node ./bin/run modernizr",
    "build:lodash": "lodash include=\"each,filter,map,debounce,template,sortBy\" -o assets/javascript/vendor/lodash.custom.js",
    "analyse:eslint": "eslint assets/javascript/**/*.js",
    "precache": "node ./bin/run sw-precache",
    "brotli": "node ./bin/run brotli",
    "test": "npm run analyse:eslint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": ["> 2%", "ie >= 9"]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "autotrack": "2.4.0",
    "awesomplete": "^1.1.1",
    "blueimp-file-upload": "^9.18.0",
    "bootstrap": "4.0.0-alpha.6",
    "bootstrap-datepicker": "^1.6.4",
    "core-js": "^2.4.1",
    "es6-promise": "^4.1.0",
    "form-serialize": "^0.7.1",
    "google-maps": "^3.2.1",
    "imagesloaded": "4.1.2",
    "jquery": "^3.2.1",
    "jump.js": "^1.0.2",
    "lodash": "~4.17.4",
    "mkdirp": "^0.5.1",
    "modernizr": "^3.5.0",
    "normalize-for-search": "^2.0.1",
    "parsleyjs": "2.7.2",
    "qs": "^6.4.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "slick-carousel": "^1.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "brotli": "1.3.2",
    "cache-loader": "^1.0.3",
    "chalk": "^1.1.3",
    "chokidar-cli": "^1.2.0",
    "cross-env": "5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "imports-loader": "^0.7.1",
    "node-notifier": "^5.1.2",
    "node-sass": "4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss": "6.0.1",
    "rimraf": "^2.6.1",
    "stylelint": "^7.10.1",
    "stylelint-config-suitcss": "^11.0.0",
    "stylelint-scss": "^1.4.4",
    "svg-sprite": "^1.3.7",
    "sw-precache": "^5.1.1",
    "webpack": "2.6.1"
  }
}
